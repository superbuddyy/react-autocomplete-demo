{
  "extends": "rackt",
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "array-bracket-spacing": 0,
    "comma-dangle": 0,
    "object-curly-spacing": 0,
    "quotes": 0,
    "semi": 0,
    "space-before-blocks": 0,
    "space-before-function-paren": 0
  }
}
